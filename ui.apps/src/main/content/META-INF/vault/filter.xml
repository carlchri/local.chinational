<workspaceFilter version="1.0">
    <filter root="/apps/chinational">
        <exclude pattern="/apps/chinational(/.*)?"/>
        <include pattern="/apps/chinational/clientlibs(/.*)?"/>
        <include pattern="/apps/chinational/components(/.*)?"/>
        <include pattern="/apps/chinational/templates(/.*)?"/>
        <include pattern="/apps/chinational/granite(/.*)?"/>
    </filter>
    <filter root="/apps/sling/servlet">
        <exclude pattern="/apps/sling/servlet(/.*)?"/>
        <include pattern="/apps/sling/servlet/errorhandler(/.*)?"/>
    </filter>
    <filter root="/etc/config/chinational">
        <exclude pattern="/etc/config/chinational(/.*)?"/>
        <include pattern="/etc/config/chinational/components(/.*)?"/>
    </filter>
    <filter root="/etc/designs/chicommon">
        <exclude pattern="/etc/designs/chicommon(/.*)?"/>
        <include pattern="/etc/designs/chicommon/images(/.*)?"/>
    </filter>
    <filter root="/conf/chinational">
        <exclude pattern="/conf/chinational(/.*)?"/>
        <include pattern="/conf/chinational/.content.xml"/>
        <include pattern="/conf/chinational/settings(/.*)?"/>
    </filter>
</workspaceFilter>
