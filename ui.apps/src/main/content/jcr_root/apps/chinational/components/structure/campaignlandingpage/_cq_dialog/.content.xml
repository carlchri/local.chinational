<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <basic jcr:primaryType="nt:unstructured"/>
                    <advanced
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <socialmedia
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <personalization
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}false"/>
                    <permissions
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <chinational
                        jcr:primaryType="nt:unstructured"
                        jcr:title="CHI Page Properties"
                        sling:orderBefore="advanced"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <herocomponent
                                        jcr:primaryType="nt:unstructured"
                                        disabled="false"/>
                                    <breadcrumb
                                        jcr:primaryType="nt:unstructured"
                                        disabled="true"/>
                                    <subnavigation
                                        jcr:primaryType="nt:unstructured"
                                        disabled="true"/>
                                    <rightrail
                                        jcr:primaryType="nt:unstructured"
                                        disabled="true"/>
                                    <hidePageTitle
                                        jcr:primaryType="nt:unstructured"
                                        disabled="true"/>
                                </items>
                            </column>
                        </items>
                    </chinational>
                    <campaigndetail
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Campaign Page Properties"
                        sling:orderBefore="advanced"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <buttonLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        emptyText="Enter Button Label"
                                        fieldDescription="Enter Label for the button in Campaign landing Page Header"
                                        fieldLabel="Button Label"
                                        name="./linkText"
                                        renderReadOnly="{Boolean}true"/>
                                    <linkUrl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Link to pages on this site. &#xa;For External Links, use format http(s)://www.example.com"
                                        fieldLabel="Link URL"
                                        name="./linkUrl"
                                        rootPath="/content"/>
                                    <target
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="Selecting this checkbox will open link in new window."
                                        name="./targetBlank"
                                        text="Select to Open Link in New Window"
                                        uncheckedValue="false"
                                        value="true"/>
                                </items>
                            </column>
                        </items>
                    </campaigndetail>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
