<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <basic jcr:primaryType="nt:unstructured"/>
                    <advanced
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <socialmedia
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <cloudservices
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <personalization
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <permissions
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <chinational
                        jcr:primaryType="nt:unstructured"
                        jcr:title="CHI Page Properties"
                        sling:orderBefore="advanced"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <herocomponent
                                        jcr:primaryType="nt:unstructured"
                                        disabled="true"/>
                                    <subnavigation
                                        jcr:primaryType="nt:unstructured"
                                        disabled="true"/>
                                    <rightrail
                                        jcr:primaryType="nt:unstructured"
                                        disabled="false"/>
                                </items>
                            </column>
                        </items>
                    </chinational>
                    <directoryprofile
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Directory Profile Properties"
                        sling:orderBefore="advanced"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <physicianNameAndDegree
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        emptyText="Enter Physician name and degree"
                                        fieldDescription="Enter Physician name and degree"
                                        fieldLabel="Physician Name and Degree"
                                        name="./physicianName"
                                        required="{Boolean}true"/>
                                    <expertise
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        emptyText="Max 350 characters allowed."
                                        fieldDescription="Enter the expertise areas of Physician. Max 350 characters allowed."
                                        fieldLabel="Physician Expertise"
                                        maxlength="{Long}350"
                                        name="./physicianExpertise"
                                        required="{Boolean}true"/>
                                    <physicianImageSrc
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter source for Physician Image to be displayed on the Page."
                                        fieldLabel="Enter Physician Image Src"
                                        name="./imageSrc"
                                        rootPath="/content/dam"/>
                                    <physicianImageNotReqd
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="Select this checkbox if Physician Image is not required"
                                        name="./physicianImageNotReqd"
                                        text="Select if Physician Image is not Required"
                                        uncheckedValue="false"
                                        value="true"/>
                                    <appointmentButtonNotReqd
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="Selecting this checkbox if Request Appointment Button is not required"
                                        name="./apointmentButtonNotReqd"
                                        text="Select if Request Appointment Button is not required"
                                        uncheckedValue="false"
                                        value="true"/>
                                </items>
                            </column>
                        </items>
                    </directoryprofile>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
