<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <basic jcr:primaryType="nt:unstructured"/>
                    <advanced
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <socialmedia
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <personalization
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}false"/>
                    <permissions
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <chinational
                        jcr:primaryType="nt:unstructured"
                        jcr:title="CHI Page Properties"
                        sling:orderBefore="advanced"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <herocomponent
                                        jcr:primaryType="nt:unstructured"
                                        disabled="true"/>
                                    <breadcrumb
                                        jcr:primaryType="nt:unstructured"
                                        disabled="true"/>
                                    <subnavigation
                                        jcr:primaryType="nt:unstructured"
                                        disabled="true"/>
                                    <rightrail
                                        jcr:primaryType="nt:unstructured"
                                        disabled="true"/>
                                    <hidePageTitle
                                        jcr:primaryType="nt:unstructured"
                                        disabled="true"/>
                                </items>
                            </column>
                        </items>
                    </chinational>
                    <defaultValues
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Default Values"
                        sling:orderBefore="advanced"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <newsPath
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Default News Path"
                                        fieldLabel="Template to serve as basis for News tag landing pages"
                                        name="./defaultNewsPath"
                                        rootPath="/content"/>
                                    <blogsPath
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Default News Path"
                                        fieldLabel="Template to serve as basis for Blog tag landing pages"
                                        name="./defaultBlogsPath"
                                        rootPath="/content"/>
                                    <newsTileImageSrc
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Default News Tile/Featured Image Src"
                                        fieldLabel="Default News Tile/Featured Image Src"
                                        name="./defaultNewsTileImgSrc"
                                        rootPath="/content/dam"/>
                                    <blogsTileImageSrc
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Default Blogs Tile/Featured Image Src"
                                        fieldLabel="Default Blogs Tile/Featured Image Src"
                                        name="./defaultBlogsTileImgSrc"
                                        rootPath="/content/dam"/>
                                    <defaultMembersPhotoSrc
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Default Members Tile Photo Src"
                                        fieldLabel="Default Members Tile Photo Src"
                                        name="./defaultMembersPhotoSrc"
                                        rootPath="/content/dam"/>
                                    <siteExternailizer
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Used by admin to define site domain reference for AEM"
                                            fieldLabel="Site Externalizer (Admin field - do not modify)"
                                            name="./siteExternailizer"/>
                                </items>
                            </column>
                        </items>
                    </defaultValues>
                    <chianalytics
                        jcr:primaryType="nt:unstructured"
                        jcr:title="CHI Analytics"
                        sling:orderBefore="advanced"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <snippets
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldDescription="Add analytic snippets here"
                                        fieldLabel="Analytic Snippets"
                                        name="./analyticsnippets"/>
                                    <gsearchid
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Google Search Console to track how Google is indexing your website"
                                        fieldLabel="Google Search Console ID"
                                        name="./gsearchid"/>
                                </items>
                            </column>
                        </items>
                    </chianalytics>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
